    "prettier": "^1.15.3",
    "tsify": "^4.0.1",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.3.3333",
    "typescript-tslint-plugin": "^0.3.1",
    "watchify": "^3.11.1"
  }
